<p id="notice"><%= notice %></p>

<h1>Sustratos</h1>

<table>
  <thead>
    <tr>
      <th>Idsustrato</th>
      <th>Sustrato</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sustratos.each do |sustrato| %>
      <tr>
        <td><%= sustrato.idSustrato %></td>
        <td><%= sustrato.sustrato %></td>
        <td><%= link_to 'Show', sustrato %></td>
        <td><%= link_to 'Edit', edit_sustrato_path(sustrato) %></td>
        <td><%= link_to 'Destroy', sustrato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sustrato', new_sustrato_path %>
